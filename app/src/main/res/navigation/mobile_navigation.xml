<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.daisy_mobile.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_order"
        android:name="com.example.daisy_mobile.ui.order.OrderFragment"
        android:label="@string/title_order"
        tools:layout="@layout/fragment_order" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.daisy_mobile.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.example.daisy_mobile.ui.favorite.FavouriteFragment"
        android:label="@string/title_favorite"
        tools:layout="@layout/favourite_fragment" />

</navigation>